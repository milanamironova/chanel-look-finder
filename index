// --- Utils ---
function normalize(t){
 if(!t) return "";
 return t.toLowerCase().replace(/[^\wÃ€-Å¾\s]/g, " ").replace(/\s+/g, " ").trim();
}
function scoreMatch(q, t){
 let qTok = new Set(normalize(q).split(" "));
 let tTok = new Set(normalize(t).split(" "));
 if(qTok.size === 0 || tTok.size === 0) return 0;
 let inter = [...qTok].filter(x => tTok.has(x));
 return inter.length / qTok.size;
}
function inferType(show){
 let s = show.toLowerCase();
 if(s.includes('haute couture')) return 'haute couture';
 if(s.includes('pret') || s.includes('prÃªt') || s.includes('rtw')) return 'rtw';
 return '';
}
function inferSeason(show){
 let s = show.toLowerCase();
 if(['printemps','Ã©tÃ©','spring','summer'].some(x=>s.includes(x))) return 'ss';
 if(['automne','hiver','autumn','winter'].some(x=>s.includes(x))) return 'aw';
 return '';
}

// --- Preprocess DF ---
function prepare(){
 df = df.map((r,i) => {
   r.combined = normalize(r["GARMENTS/OUTFITS"]) + " " + normalize(r["PRIMARY COLOURS"]) + " " + normalize(r["ACCESSORIES"]);
   r.TYPE = inferType(r.SHOW || "");
   r.SEASON = inferSeason(r.SHOW || "");
   r.index = i;
   return r;
 });
}

// --- Store options ---
const stores = {
 "Chanel Rue Cambon â€“ Paris": { link:"https://www.chanel.com", map:"https://maps.google.com/?q=Chanel+31+Rue+Cambon+Paris" },
 "Chanel Ginza â€“ Tokyo": { link:"https://www.chanel.com", map:"https://maps.google.com/?q=Chanel+Ginza+Tokyo" },
 "Chanel New Bond Street â€“ London": { link:"https://www.chanel.com", map:"https://maps.google.com/?q=Chanel+New+Bond+Street+London" },
 "Chanel Soho â€“ New York": { link:"https://www.chanel.com", map:"https://maps.google.com/?q=Chanel+Soho+New+York" }
};

// --- UI helpers ---
const out = document.getElementById('output');
function renderResults(list){
 return `<select id="results" size="10">` +
   list.map(r => `<option value="${r.index}">${r.label}</option>`).join('') +
   `</select><br><button id="chooseBtn">That's the one</button> <button id="restartBtn">Restart</button>`;
}
function restart(){ out.innerHTML = ""; }

// --- Search ---
document.getElementById('searchBtn').onclick = function(){
 if(df.length === 0){ out.innerHTML = "Load your df JSON into the script first."; return; }
 let q = document.getElementById('desc').value.trim();
 let typeFilter = document.getElementById('typeSelect').value;
 let seasonFilter = document.getElementById('seasonSelect').value;
 if(!q){ out.innerHTML = "Please describe the item first."; return; }

 df.forEach(r => r.score = scoreMatch(q, r.combined));
 let candidates = df.filter(r => r.score > 0);

 if(typeFilter !== "idk") candidates = candidates.filter(r => r.TYPE === typeFilter);
 if(seasonFilter !== "idk") candidates = candidates.filter(r => r.SEASON === seasonFilter);

 candidates.sort((a,b)=>b.score - a.score);
 candidates = candidates.slice(0,20);

 if(candidates.length === 0){ out.innerHTML = "No close matches found."; return; }

 let list = candidates.map(r=>({
   index: r.index,
   label:`${r.SHOW} | Look ${r["LOOK NB"]} | ${r["GARMENTS/OUTFITS"]} | ${r["PRIMARY COLOURS"]} | ${r["ACCESSORIES"]} (score ${r.score.toFixed(2)})`
 }));

 out.innerHTML = renderResults(list);
 document.getElementById('chooseBtn').onclick = ()=>chooseLook(candidates);
 document.getElementById('restartBtn').onclick = restart;
};

// --- Choose look ---
function chooseLook(cands){
 let sel = document.getElementById('results').value;
 let row = cands.find(r => r.index == sel);
 if(!row){ out.innerHTML = "No look selected."; return; }

 out.innerHTML = `
   <h3>ðŸŽ‰ You selected this look:</h3>
   SHOW: ${row.SHOW}<br>
   LOOK NB: ${row["LOOK NB"]}<br>
   GARMENTS/OUTFITS: ${row["GARMENTS/OUTFITS"]}<br>
   PRIMARY COLOURS: ${row["PRIMARY COLOURS"]}<br>
   ACCESSORIES: ${row["ACCESSORIES"]}<br><br>
   <h4>Choose a store:</h4>
   <select id="storeSelect" size="6">
     ${Object.keys(stores).map(s=>`<option>${s}</option>`).join('')}
   </select><br>
   <button id="confirmStore">Select Store</button>
   <button id="restartBtn">Restart</button>
 `;

 document.getElementById('confirmStore').onclick = confirmStore;
 document.getElementById('restartBtn').onclick = restart;
}

// --- Store confirm ---
function confirmStore(){
 let store = document.getElementById('storeSelect').value;
 let info = stores[store];
 out.innerHTML = `
   <h3>ðŸ› Store selected:</h3>
   ${store}<br><br>
   <a href="${info.link}" target="_blank">Store Website</a><br><br>
   <a href="${info.map}" target="_blank">Open Map</a><br><br>
   <button id="restartBtn">Restart</button>
 `;
 document.getElementById('restartBtn').onclick = restart;
}

// --- Initialize ---
prepare();
</script>
</body>
</html>
